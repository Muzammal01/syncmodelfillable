{
    "name": "muzammal/syncmodelfillable",
    "version": "1.5.0",
    "description": "A Laravel package to synchronize model fillable fields with migration columns using commands",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Muzammal",
            "email": "muzidev55@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "illuminate/support": "^8.0|^9.0|^10.0|^11.0|^12.0",
        "illuminate/console": "^8.0|^9.0|^10.0|^11.0|^12.0",
        "illuminate/filesystem": "^8.0|^9.0|^10.0|^11.0|^12.0"
    },
    "autoload": {
        "psr-4": {
            "Muzammal\\Syncmodelfillable\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Muzammal\\Syncmodelfillable\\SyncModelFillableServiceProvider"
            ]
        }
    },
    "require-dev": {
        "orchestra/testbench": "^6.0|^7.0|^8.0|^9.0" 
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
